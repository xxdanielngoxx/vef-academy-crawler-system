<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
    http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">
    <changeSet id="create-table-url-v0" author="danielngo1998@gmail.com">
        <preConditions onFail="MARK_RAN">
            <not>
                <tableExists tableName="seed_details"/>
            </not>
        </preConditions>
        <createTable tableName="seed_details">
            <column name="seed_id" type="uuid">
                <constraints nullable="false" primaryKey="true" primaryKeyName="pk_seed_details"/>
            </column>
            <column name="url_id" type="nvarchar(255)">
                <constraints nullable="false" primaryKey="true" primaryKeyName="pk_seed_details"/>
            </column>
            <column name="created_date" type="timestamp">
            </column>
        </createTable>
        <addForeignKeyConstraint
                baseTableName="seed_details"
                baseColumnNames="seed_id"
                constraintName="fk_seed_details_seed_urls"
                referencedTableName="seed_urls"
                referencedColumnNames="id"
        />
        <addForeignKeyConstraint
                baseTableName="seed_details"
                baseColumnNames="url_id"
                constraintName="fk_seed_details_urls"
                referencedTableName="urls"
                referencedColumnNames="url"
        />
    </changeSet>
</databaseChangeLog>
<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
    http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">
    <changeSet id="create-table-url-v0" author="danielngo1998@gmail.com">
        <preConditions onFail="MARK_RAN">
            <not>
                <tableExists tableName="seed_urls"/>
            </not>
        </preConditions>
        <createTable tableName="seed_urls">
            <column name="id" type="uuid">
                <constraints primaryKey="true" nullable="false" unique="true"/>
            </column>
            <column name="url" type="nvarchar(255)">
                <constraints nullable="false"/>
            </column>
        </createTable>
        <createIndex tableName="seed_urls" indexName="index_url">
            <column name="url" descending="true"/>
        </createIndex>
    </changeSet>
</databaseChangeLog>